{
  "name": "mathvr",
  "version": "0.1.6",
  "description": "Math in VR.",
  "dependencies": {
    "aframe": "^0.3.2",
    "aframe-extras": "^2.4.0",
    "aframe-bmfont-text-component": "^0.1.4",
    "aframe-keyboard": "0.0.1",
    "aframe-look-at-component": "^0.1.2",
    "aframe-react": "^3.4.0",
    "aframe-sprite-component": "^0.1.0",
    "aframe-text-component": "^0.3.0",
    "babel-polyfill": "^6.3.14",
    "mathbox": "git+https://git@gitgud.io/michaltakac/mathbox.git",
    "mathjs": "^3.6.0"
  },
  "scripts": {
    "build": "webpack",
    "codecov": "codecov",
    "preghpages": "npm run build && rm -rf gh-pages && cp -r src gh-pages && cp index.html build/bundle.js gh-pages",
    "ghpages": "npm run preghpages && ghpages -p gh-pages",
    "deploy": "npm run ghpages",
    "start": "webpack-dev-server --content-base src --progress --watch --history-api-fallback --inline --hot --host 0.0.0.0 --port 9090",
    "lint:eslint": "./node_modules/.bin/eslint src || true",
    "test": "karma start ./tests/karma.conf.js",
    "test:firefox": "karma start ./tests/karma.conf.js --browsers Firefox",
    "test:chrome": "karma start ./tests/karma.conf.js --browsers Chrome",
    "test:ci": "TEST_ENV=ci karma start ./tests/karma.conf.js --single-run --browsers Firefox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DimensionLab/mathvr.git"
  },
  "keywords": [
    "mathvr",
    "aframe",
    "react",
    "vr",
    "math",
    "a-frame",
    "mozvr",
    "webvr"
  ],
  "engines": {
    "node": ">= 0.12.7",
    "npm": "^2.12.1"
  },
  "author": "Michal Takac <hello@michaltakac.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DimensionLab/mathvr/issues"
  },
  "homepage": "https://github.com/DimensionLab/mathvr#readme",
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-core": "^6.3.15",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.13",
    "chai": "^3.5.0",
    "chai-shallow-deep-equal": "^1.4.4",
    "codecov": "^1.0.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.23.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^11.2.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "exorcist": "^0.4.0",
    "file-loader": "^0.9.0",
    "ghpages": "0.0.3",
    "husky": "^0.11.9",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chai-shallow-deep-equal": "0.0.4",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-sinon-chai": "^1.2.4",
    "karma-webpack": "^1.8.0",
    "lolex": "^1.5.1",
    "mocha": "^3.1.2",
    "mozilla-download": "^1.1.1",
    "open": "0.0.5",
    "replace": "^0.3.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-server": "^1.14.0"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "src"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "extends": [
      "airbnb"
    ],
    "globals": {
      "browser": true,
      "node": true,
      "mocha": true,
      "es6": true,
      "document": true,
      "window": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "import/imports-first": 0,
      "import/newline-after-import": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-named-as-default": 0,
      "import/no-unresolved": 2,
      "import/prefer-default-export": 0,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "jsx-a11y/aria-props": 2,
      "jsx-a11y/heading-has-content": 0,
      "jsx-a11y/href-no-hash": 2,
      "jsx-a11y/label-has-for": 2,
      "jsx-a11y/mouse-events-have-key-events": 2,
      "jsx-a11y/role-has-required-aria-props": 2,
      "jsx-a11y/role-supports-aria-props": 2,
      "max-len": 0,
      "newline-per-chained-call": 0,
      "no-console": 1,
      "no-use-before-define": 0,
      "prefer-template": 2,
      "class-methods-use-this": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-first-prop-new-line": [
        2,
        "multiline"
      ],
      "react/jsx-filename-extension": 0,
      "react/jsx-no-target-blank": 0,
      "react/require-extension": 0,
      "react/self-closing-comp": 0,
      "require-yield": 0
    }
  }
}
